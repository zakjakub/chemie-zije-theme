{
  "name": "zakjakub/chemie-zije-theme",
  "description": "Theme for \"CHEMIE Å½IJE!\" WordPress website.",
  "type": "wordpress-theme",
  "license": "proprietary",
  "minimum-stability": "dev",
  "authors": [
    {
      "email": "mail@jakubzak.eu",
      "name": "Jakub ZAK",
      "homepage": "https://jakubzak.cz",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=8.0",
    "timber/timber": "^2.0.0-rc.1",
    "wpackio/enqueue": "^3.5.0",
    "hellonico/timber-dump-extension": "dev-master"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.6",
    "roots/wordpress": "^6.3",
    "automattic/wordbless": "^0.4",
    "htmlburger/carbon-fields": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Zakjakub\\ChemieZijeTheme\\": "src"
    },
    "classmap": [
      ""
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "composer/installers": true,
      "roots/wordpress-core-installer": true
    }
  },
  "extra": {
    "installer-paths": {
      "vendor/automattic/wordbless/": [
        "automattic/wordbless"
      ]
    },
    "wordpress-install-dir": "wordpress"
  }
}
